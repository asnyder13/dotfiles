#!/usr/bin/env bash

# DOTFILES and LINKS need to match indecies.
DOTFILES=(
	.zshrc
	init.lua
	.commonrc
	.bash_aliases
	.bashrc
	vimrc
	.rubocop.yml
	util.lua
	lsp.lua
)
DOTFILE_LINKS=(
	$HOME              # .zshrc
	$HOME/.config/nvim # init.lua
	$HOME              # .commonrc
	$HOME              # .bash_aliases
	$HOME              # .bashrc
	$HOME/.vim         # vimrc
	$HOME              # .rubocop.yml
	$HOME/.config/nvim/lua # util.lua
	$HOME/.config/nvim/lua # lsp.lua
)
